<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="DM2.Ent.Client.Views.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <!--界面呈现设置-->
    <add key="DefaultThem" value="White" />
    <!--<add key="sqlConnectionStr" value="Data Source=192.168.1.118;Initial Catalog=soulieren0;User Id=root;Password=Aa123456;"/>-->
    <add key="sqlConnectionStr" value="Data Source=localhost;Initial Catalog=soulieren;User Id=root;Password=;Convert Zero Datetime=True;Allow Zero Datetime=True;" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <!--此配置用于监控统计程序的网络流量-->
  <system.net>
    <settings>
      <performanceCounters enabled="true" />
    </settings>
  </system.net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <!--用户初始配置信息-->
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
  <userSettings>
    <DM2.Ent.Client.Views.Properties.Settings>
      <setting name="WindowsPosition" serializeAs="String">
        <value>0,0,0,0</value>
      </setting>
      <setting name="IsMaximized" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ContentTopRow" serializeAs="String">
        <value>0.5*</value>
      </setting>
      <setting name="ContentBottomRow" serializeAs="String">
        <value>0.5*</value>
      </setting>
      <setting name="ContentLeftColumn" serializeAs="String">
        <value>0.5*</value>
      </setting>
      <setting name="ContentRightColumn" serializeAs="String">
        <value>0.5*</value>
      </setting>
    </DM2.Ent.Client.Views.Properties.Settings>
  </userSettings>
</configuration>