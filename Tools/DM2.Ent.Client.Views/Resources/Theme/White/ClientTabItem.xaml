    <ImageBrush x:Key="248tabItemNormalHeader"
                Stretch="Fill"
                ImageSource="/Resources/Images/White/ClientTabItem/248_25TabItemNormal.png" />
    <ImageBrush x:Key="248tabItemSelectedHeader"
                Stretch="Fill"
