            <SolidColorBrush x:Key="groupBoxHeaderBorder"
                             PresentationOptions:Freeze="True"
                             Color="#999999" />
            <!--GroupBox头背景色-->
            <SolidColorBrush x:Key="groupBoxHeaderBG"
                             PresentationOptions:Freeze="True"
                             Color="#f0f0f0" />
            
            
            <!--[[[[ GroupBox End ]]]]-->
            <!--[[[[ 弹出对话框公共样式 End ]]]]-->
