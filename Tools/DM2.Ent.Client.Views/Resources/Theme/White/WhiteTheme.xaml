            <SolidColorBrush x:Key="DockSiteBG">#838383</SolidColorBrush>
            <!--主页面左右两边竖条的渐变颜色值-->
            <LinearGradientBrush x:Key="MainWindowBorderBG"
                                 EndPoint="0.5,1"
                                 StartPoint="0.5,0">
                <GradientStop Color="#FFD7D7D7"
                              Offset="0.997" />
                <GradientStop Color="#FFE4E4E4" />
            </LinearGradientBrush>
            <!--全局控件背景色-->
            <Color x:Key="ControlBackgroundColor">#FFFFFF</Color>
