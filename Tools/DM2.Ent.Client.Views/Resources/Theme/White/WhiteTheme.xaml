    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary>
            
            <!--窗体边框颜色-->
            <SolidColorBrush x:Key="WindowBorderBrush">#646464</SolidColorBrush>
            <!--字体颜色-->
            <SolidColorBrush x:Key="FontColor">Black</SolidColorBrush>
            <!--禁用字体颜色-->
